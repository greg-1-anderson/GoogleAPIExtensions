{
  "name": "westkingdom/google-api-extensions",
  "type": "library",
  "description": "Google Apps API Extensions",
  "homepage": "https://github.com/westkingdom/google-api-extensions",
  "license": "MIT",
  "authors": [
    {
      "name": "Greg Anderson",
      "email": "greg.1.anderson@greeknowe.org",
      "homepage": "https://www.drupal.org/u/greg.1.anderson",
      "role": "Developer"
    }
  ],
  "require": {
    "google/apiclient": "1.0.*@beta",
    "symfony/yaml": "~2.2"
  },
  "require-dev": {
    "phpunit/phpunit": ">=3.5",
    "phpspec/prophecy-phpunit": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Westkingdom\\GoogleAPIExtensions\\":    "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Westkingdom\\GoogleAPIExtensions\\":    "tests"
    }
  },
  "extra": {
    "branch-alias": {
        "dev-master": "1.0.x-dev"
    }
  }
}
